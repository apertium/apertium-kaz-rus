  <section-def-macros>
    <def-macro n="test1" npar="1">
      <let>
        <var n="POST"/>
        <lit v=""/>
      </let>
    </def-macro>

<def-macro n="build-case-ant" npar="1">
<choose> 
<when>
   <test>
     
          <equal><clip pos="1"  part="a_np"/><lit-tag  v="np.ant.f"/></equal>
     
    </test>      
          <let><var  n="case"/><lit-tag v="dat"/></let>
</when>
  <otherwise>
          <let>
            <var n="case"/><lit-tag v="acc"/>
          </let>
        </otherwise>
</choose>

</def-macro>

<def-macro n="change-case-upper" npar="1">  <!--change case to upper for first word-->

       <modify-case>
        <clip pos="1" part="lem"/>
          <lit v="Aa"/>
        </modify-case>
             <let>
        <clip pos="1"  part="lem"/>
		<lit v="Aa"/>
	      </let>
      </def-macro>
<def-macro n="build-possessive" npar="1">
<choose> 
<when>
   <test>
      <and>
          <equal><clip pos="1"  part="a_pers"/><lit-tag  v="p1"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="sg"/></equal>
       </and>
    </test>      
          <let><var  n="possessive"/><lit-tag v="px1sg"/></let>
</when>


<when>
   <test>
      <and>
          <equal><clip  pos="1"  part="a_pers"/><lit-tag  v="p1"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="pl"/></equal>
       </and>
    </test>      
          <let><var  n="possessive"/><lit-tag v="px1pl"/></let>
</when>
<when>
   <test>
      <and>
          <equal><clip pos="1"  part="a_pers"/><lit-tag  v="p2"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="sg"/></equal>
	
       </and>
    </test>      
<let><var  n="possessive"/><lit-tag v="px2sg"/></let>

</when>
<when>
   <test>
      <and>
          <equal><clip pos="1"  part="a_pers"/><lit-tag  v="p2"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="sg"/></equal>
	
       </and>
    </test>      
<let><var  n="possessive"/><lit-tag v="px2sg"/></let>

</when>
<when>
   <test>
          <equal><clip  pos="1"  part="a_pers"/><lit-tag  v="p3"/></equal>
    </test>      
          <let><var  n="possessive"/><lit-tag v="px3sp"/></let>
</when>
<otherwise>
          <let><var  n="possessive"/><lit-tag v="PXD"/></let>
</otherwise>

</choose>

</def-macro>

  </section-def-macros>



  <section-rules>


<rule comment="RULE: VP PP → PP VP"> <!-- Aliya 28.06.17 -->
     <pattern>
       <pattern-item n="VP"/>
       <pattern-item n="PP"/>
     </pattern>
     <action>
      <out>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk> 
          <b/>
          <chunk>
            <clip pos="1" part="lem"/>
            <lit-tag v="VP"/>
                        <lit-tag v="sg"/> <!-- agree number -->
                        <lit-tag v="p3"/> <!-- agree person -->
            <clip pos="1" part="a_tense"/>
            <clip pos="1" part="a_possessive"/> 
            <clip pos="1" part="a_pass"/>
            <clip pos="1" part="a_neg"/>
            <clip pos="1" part="chcontent"/>
          </chunk> 
      </out>
     </action>
    </rule>


    <rule comment="NEW RULE: VP NP → NP VP"> <!-- Aliya 28.06.17 -->
    <!--VP NP можно выбрать несколько книг -->
     <pattern>
       <pattern-item n="VP"/>
       <pattern-item n="NP"/>
     </pattern>
     <action>
      <out>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="1" part="lem"/>
            <lit-tag v="VP"/>
            <clip pos="1" part="a_nbr"/> <!-- agree number -->
            <clip pos="1" part="a_pers"/> <!-- agree person -->
            <clip pos="1" part="a_tense"/>
            <clip pos="1" part="a_possessive"/> 
            <clip pos="1" part="a_pass"/>
            <clip pos="1" part="a_neg"/>
            <clip pos="1" part="chcontent"/>
          </chunk> 
      </out>
     </action>
    </rule>

