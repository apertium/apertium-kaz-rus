    <def-cat n="NP-pron">  
      <cat-item tags="NP-pron"/>
      <cat-item tags="NP-pron.*"/>
    </def-cat>
    <def-cat n="AdjP">
      <cat-item tags="AdjP"/>
      <cat-item tags="AdjP.*"/>
      <cat-item tags="SupP"/>
      <cat-item tags="SupP.*"/>
    </def-cat>
<def-cat n="VP_wantto">
      <cat-item tags="VP_wantto.*"/>
<cat-item tags="VP_wantto"/>
    </def-cat>
    <def-cat n="VP_have">
      <cat-item tags="VP-vbhaver"/>
      <cat-item tags="VP-vbhaver.*"/>
    </def-cat>
    <def-cat n="AdvP">
      <cat-item tags="AdvP"/>
      <cat-item tags="AdvP.*"/>
    </def-cat>
    <def-cat n="CNJ">
