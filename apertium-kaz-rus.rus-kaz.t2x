    <def-cat n="VP">
      <cat-item tags="VP"/>
      <cat-item tags="VP.*"/>
    </def-cat>
    <def-cat n="VP-neg">
      <cat-item tags="VP_neg"/>
      <cat-item tags="VP_neg.*"/>
    </def-cat>
    <def-cat n="PP">
