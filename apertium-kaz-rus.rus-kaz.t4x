    <rule comment="pronoun">
    <pattern>
       <pattern-item n="perspron"/>
    </pattern>
    <action>
     
 <choose>
          <when>
            <test>
<and>
                 <equal>
                     <clip pos="1" side="sl" part="nbr"/>
                     <lit-tag v="ND"/>
                 </equal>
<equal>
                     <clip pos="1" side="sl" part="pron"/>
                     <lit-tag v="ref"/>
                 </equal>
</and>
             </test>
            <let><clip pos="1" side="tl" part="nbr"/><lit v=""/></let>
          </when>
        </choose>
        <choose>
          <when>
            <test>
                 <equal>
                     <clip pos="1" side="sl" part="nbr"/>
                     <lit-tag v="ND"/>
                 </equal>
             </test>
            <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></let>
          </when>
        </choose>

  	
        <choose>
          <when>
            <test><equal><clip pos="1" side="sl" part="poss"/><lit-tag v="PXD"/></equal></test>
            <let><clip pos="1" side="tl" part="poss"/><lit v=""/></let>
          </when>
        </choose>
        
 <choose>
          <when>
            <test><equal><clip pos="1" side="sl" part="case"/><lit-tag v="CD"/></equal></test>
            <let><clip pos="1" side="tl" part="case"/><lit-tag v="nom"/></let>
          </when>
        </choose>
        <out>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
        </out>
      </action>
    </rule>
    
    <rule comment="present-tense copula hack"> <!-- MLF 20130605 not working yet! -->
      <pattern>
        <pattern-item n="nomobject"/>
        <pattern-item n="shortcop"/>
      </pattern>
      <action>
     <choose>
     <when>
        <test>
          <equal>
             <clip pos="1" part="nbr" side="sl"/>
             <lit-tag v="sg"/>
          </equal>
        </test>
        <let>
           <clip pos="1" part="nbr" side="tl"/>
           <lit v=""/>
        </let>
     </when>
     </choose>
 <choose>
     <when>
        <test>
          <equal>
             <clip pos="2" part="a_cop" side="sl"/>
             <lit-tag v="cop"/>
          </equal>
        </test>
        <let>
           <clip pos="2" part="a_evid" side="tl"/>
           <lit v=""/>
        </let>
     </when>
     </choose>
     <choose>
     <when>
        <test>
          <equal>
             <clip pos="1" part="poss" side="sl"/>
             <lit-tag v="PXD"/>
          </equal>
        </test>
        <let>
           <clip pos="1" part="poss" side="tl"/>
           <lit v=""/>
        </let>
     </when>

     </choose>  
 <choose>
     <when>
        <test>

<and>
          <equal>
             <clip pos="1" part="case" side="sl"/>
             <lit-tag v="nom"/>
          </equal>
 <equal>
             <clip pos="1" part="adj" side="sl"/>
             <lit-tag v="adj"/>
          </equal>
</and>
        </test>
        <let>
           <clip pos="1" part="case" side="tl"/>
           <lit v=""/>
        </let>
     </when>

     </choose>      
     <!--<choose>
       <when>
         <test><equal><clip pos="2" side="sl" part="a_pers"/><lit-tag v="p2"/></equal></test> 
            <let><clip pos="2" side="tl" part="a_pers"/><lit-tag v="p2.frm"/></let>
          </when>
 	</choose>--> 
    <choose>
          <when>
            <test><equal><clip pos="1" side="sl" part="a_pers"/><lit-tag v="p3"/></equal></test>
            <let><clip pos="1" side="tl" part="a_pers"/><lit v=""/></let>
          </when>
        </choose>
  <choose>
          <when>
            <test><equal><clip pos="2" side="sl" part="tense"/><lit-tag v="past"/></equal></test>
            <let><clip pos="2" side="tl" part="tense"/><lit v=""/></let>
          </when>
        </choose>
  <choose>
     <when>
        <test>
          <equal>
             <clip pos="2" part="nbr" side="sl"/>
             <lit-tag v="ND"/>
          </equal>
        </test>
        <let>
           <clip pos="1" part="nbr" side="tl"/>
           <lit v="sg"/>
        </let>
<let>
           <clip pos="1" part="a_pers" side="tl"/>
           <lit v="p3"/>
        </let>
     </when>
     </choose>

<choose>
     <when>
        <test>
          <equal>
             <clip pos="1" part="a_post" side="sl"/>
             <lit-tag v="post"/>
          </equal>
        </test>
        <out>
          <lu>
           <clip pos="1" side="tl" part="whole"/>
         
          </lu>

        </out>
     </when>

      <otherwise>
        <out>
          <lu>
           <clip pos="1" side="tl" part="whole"/>
           <lit v="+"/>
           <clip pos="2" side="tl" part="whole"/>
          </lu>

        </out>
      </otherwise>
     </choose>
      </action>  
    </rule>

     
