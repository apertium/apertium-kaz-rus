            <let><clip pos="1" side="tl" part="poss"/><lit-tag v="px2sg.frm"/></let>
          </when>
        </choose>-->
     <choose>
          <when>
            <test>
                 <equal>
                     <clip pos="1" side="sl" part="tense"/><lit-tag v="ger_past"/>
                  </equal>
             </test>
            <let><clip pos="1" side="tl" part="nbr"/><lit v=""/></let>
           <let><clip pos="1" side="tl" part="a_pers"/><lit v=""/></let>
          </when>
        </choose>



<!--<choose>
          <when>
            <test>
<or>
                 <equal>
                     <clip pos="1" side="sl" part="tense"/><lit-tag v="subs"/>
                  </equal>
<equal>
