                  </equal>
</or>
             </test>
            <let><clip pos="1" side="tl" part="tense"/><lit-tag v="ger"/></let>
          </when>
        </choose>
-->

 <choose>
          <when>
            <test>
                 <equal>
                     <clip pos="1" side="sl" part="sep"/>
                     <lit-tag v="sep"/>
                 </equal>
             </test>
            <let><clip pos="1" side="tl" part="sep"/><lit v=""/></let>
          </when>
        </choose>

        <choose>
          <when>
            <test>
                 <equal>
                     <clip pos="1" side="sl" part="nbr"/>
                     <lit-tag v="ND"/>
                 </equal>
             </test>
            <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></let>
          </when>
        </choose>
 
<choose>
          <when>
            <test>
                 <equal>
                     <clip pos="1" side="sl" part="nbr"/>
                     <lit-tag v="sp"/>
                 </equal>
             </test>
            <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></let>
          </when>
        </choose>
        <choose>
          <when>
            <test><equal><clip pos="1" side="sl" part="a_neg"/><lit-tag v="NXD"/></equal></test>
            <let><clip pos="1" side="tl" part="a_neg"/><lit v=""/></let>
          </when>
        </choose>
        <choose>
          <when>
