       <pattern-item n="v"/>
    </pattern>
    <action>
<!--
      <choose>
          <when>
            <test><equal><clip pos="1" side="sl" part="a_pers"/><lit-tag v="p2"/></equal></test>  
            <let><clip pos="1" side="tl" part="a_pers"/><lit-tag v="p2.frm"/></let>
